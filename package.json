{
  "name": "slide-starter",
  "version": "1.0.0",
  "description": "Slide Starter is a simple tool that simplifies the process of creating slide decks for mobile website and app audits through Apps Script, leveraging Google Sheets for the analysis.",
  "main": ".eslintrc.js",
  "type": "commonjs",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "karma start karma.conf.js",
